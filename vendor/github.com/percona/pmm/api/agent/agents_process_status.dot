digraph {
  node [style=filled];

  STARTING [fillcolor=green];
  RUNNING;
  BACKOFF;
  STOPPING;
  STOPPED [fillcolor=red];

  STARTING -> RUNNING;
  RUNNING -> STOPPING [label=Stop];
  STOPPING -> STOPPED;

  STARTING -> BACKOFF;
  BACKOFF -> STARTING;

  RUNNING -> BACKOFF;
  BACKOFF -> STOPPING [label=Stop];
}
